{
  "id": "com.massey-consulting.notion-sync",
  "version": "0.0.32",
  "name": "Sync to DB - A Notion integration",
  "icon": "",
  "permissions": [
    {
      "permission": "event_read",
      "purpose": "Read webhook event data"
    },
    {
      "permission": "customer_read",
      "purpose": "Read customer data"
    },
    {
      "permission": "dispute_read",
      "purpose": "Read dispute data"
    },
    {
      "permission": "invoice_read",
      "purpose": "Read invoices"
    },
    {
      "permission": "quote_read",
      "purpose": "Read quotes"
    },
    {
      "permission": "secret_write",
      "purpose": "Store credentials for API calls"
    },
    {
      "permission": "charge_read",
      "purpose": "Read charge details"
    },
    {
      "permission": "payment_method_read",
      "purpose": "Read payment methods"
    },
    {
      "permission": "payment_intent_read",
      "purpose": "Read payment intents"
    },
    {
      "permission": "credit_note_read",
      "purpose": "Read credit notes"
    },
    {
      "permission": "plan_read",
      "purpose": "Read plans, prices and products"
    },
    {
      "permission": "product_read",
      "purpose": "Read product information"
    },
    {
      "permission": "promotion_code_read",
      "purpose": "Read promotion codes"
    }
  ],
  "connect_permissions": null,
  "ui_extension": {
    "views": [
      {
        "viewport": "settings",
        "component": "AppSettings"
      }
    ],
    "content_security_policy": {
      "connect-src": [
        "https://notion.sync-to-db.com/stripe",
        "https://notion.sync-to-db.com/stripe/"
      ],
      "image-src": null,
      "purpose": ""
    }
  },
  "post_install_action": {
    "type": "settings",
    "url": ""
  },
  "constants": {
    "API_BASE": "https://notion.sync-to-db.com"
  },
  "stripe_api_access_type": "platform",
  "distribution_type": "public",
  "sandbox_install_compatible": true
}